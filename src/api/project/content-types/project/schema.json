      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "richtext"
    },
    "medias": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "couleur": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "slug": {
      "type": "uid",
      "targetField": "titre"
    },
    "rank": {
      "type": "integer",
      "default": 0
