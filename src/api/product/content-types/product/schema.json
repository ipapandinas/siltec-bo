{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Produit",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "richtext"
    },
    "medias": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "typologies": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::typology.typology",
      "mappedBy": "produit"
    },
    "rank": {
      "type": "integer",
      "default": 0
    },
    "marque": {
      "type": "string",
      "required": false
    },
    "designer": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "titre"
    },
    "collections": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::collection.collection",
      "mappedBy": "produit"
    }
  }
}
